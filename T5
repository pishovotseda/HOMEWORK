using System;

namespace T5
{
    class Program
    {
        static void Main()
        {
            int x = 1;
            int y = 1;

            while (y <= 20)
            {
                if (x % y == 0) y++;
                else { x++; y = 1; }
            }
            Console.WriteLine(x);
        }
    }
}
